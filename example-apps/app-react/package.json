{
  "name": "app-react",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "serve": "vite preview",
    "build": "yarn lib:install && yarn build:react",
    "build:react": "tsc && vite build",
    "lib:install": "yarn lib:build && yarn lib:link",
    "lib:build": "cd ../uilib && yarn lib:build && cd ../app-react",
    "lib:link": "cd ../uilib  && yarn lib:link  && cd ../app-react && yarn link @sprinteins/dpdhl-uilib",
    "postinstall": "yarn lib:link"
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "typescript": "^4.1.2",
    "vite": "^2.3.0"
  }
}