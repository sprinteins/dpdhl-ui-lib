{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "yarn --cwd ./uilib build",
    "dev": "concurrently \"yarn sb\" \"yarn lib:build:watch\" ",
    "start": "sirv public --no-clear",
    "sb": "start-storybook -p 6006 --ci -s ./uilib/src/assets",
    "build:sb": " yarn lib:build && yarn lib:link && build-storybook --static-dir ./uilib/src/assets --output-dir public-sb ",
    "link": "yarn lib:link",
    "lib:build": "yarn --cwd ./uilib install && yarn --cwd ./uilib build",
    "lib:link": "cd ./uilib && yarn link && cd .. && yarn link dpdhl-uilib",
    "lib:build:watch": "cd ./uilib && yarn build:watch",
    "postinstall": "yarn lib:link"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-svelte-csf": "^1.0.0",
    "@storybook/svelte": "^6.2.9",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.0.2",
    "react-is": "^17.0.2",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.38.1",
    "svelte-loader": "^3.1.1"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  }
}